server {
    listen 80;
    listen [::]:80;

    access_log /var/log/nginx/reverse-access.log;
    error_log /var/log/nginx/reverse-error.log;

    location / {
        proxy_pass http://127.0.0.1:{{ docker_web_app_frontend_port }};
    }

    location /api/ {
        proxy_pass http://127.0.0.1:{{ docker_web_app_api_port }}/api/;
    }
}